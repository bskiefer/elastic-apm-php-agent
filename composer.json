{
	"name": "bskiefer/elastic-apm-php-agent",
	"description": "A php 5.6 agent for Elastic APM",
	"license": "MIT",
	"require": {
		"php": "5.6.*",
		"guzzlehttp/guzzle": "6.*",
		"ramsey/uuid": "^3.7",
		"ralouphie/getallheaders": "2.0.5"
	},
	"require-dev": {
		"phpunit/phpunit": "5.*"
	},
	"suggest": {
		"ext-xdebug": "Required for processing of request headers"
	},
	"autoload": {
		"psr-4": {
			"PhilKra\\": "src/"
		},
		"files": [
			"src/Helper/Helpers.php"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"PhilKra\\Tests\\": "tests/"
		}
	},
	"config": {
		"optimize-autoloader": true
	},
	"authors": [{
		"name": "Philip Krauss",
		"email": "philip@philipkrauss.at",
		"homepage": "https://github.com/philkra/elastic-apm-agent-php"
	}, {
		"name": "George Boot",
		"email": "george@entryninja.com"
	}]
}
